{
  "compilerOptions": {
    "target": "es6",
    "module": "CommonJS",
    "baseUrl": "./src",
    "paths": {
      "utils/*": ["./utils/*"]
    },
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["**/*"],
  "exclude": ["node_modules"]
}
